    <tu>
      <tuv xml:lang="en-US">
        <seg>[Understood.]</seg>
      </tuv>
      <tuv xml:lang="fr-FR">
        <seg>[Compris.]</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="en-US">
        <seg>Run a newsfeed scan.</seg>
      </tuv>
      <tuv xml:lang="fr-FR">
        <seg>Lance une analyse des newsfeed.</seg>
